<?import javafx.scene.layout.GridPane?>

<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TableColumn?>
<GridPane fx:controller="sample.Controller"
          fx:id="gridPane" xmlns:fx="http://javafx.com/fxml"
          alignment="center" hgap="10" vgap="10">

    <TableView fx:id="tableView" prefWidth="800" prefHeight="600">
        <columns>
            <TableColumn fx:id="column1" text="省" prefWidth="200"/>
            <TableColumn fx:id="column2" text="市" prefWidth="200"/>
            <TableColumn fx:id="column3" text="县" prefWidth="200"/>
        </columns>
    </TableView>
</GridPane>